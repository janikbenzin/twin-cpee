<testset xmlns="http://cpee.org/ns/properties/2.0">
  <executionhandler>ruby</executionhandler>
  <dataelements/>
  <endpoints>
    <timeout>https-post://cpee.org/services/timeout.php</timeout>
    <dtcget>https-get://cpee.org/dtc/10/all</dtcget>
    <setmoisture>https-put://cpee.org/dtc/10/sensors/moisture</setmoisture>
  </endpoints>
  <attributes>
    <guarded>none</guarded>
    <modeltype>CPEE</modeltype>
    <guarded_id/>
    <theme>dataflow</theme>
    <info>Soil Moisture - Physical</info>
    <creator>Christine Ashcreek</creator>
    <author>Christine Ashcreek</author>
    <model_uuid>0172460e-67b2-4017-b765-414cf12577b3</model_uuid>
    <model_version/>
    <design_dir>Experiments.dir/Digital Twin Container.dir/CropIrrigation.dir/</design_dir>
    <design_stage>development</design_stage>
  </attributes>
  <description>
    <description xmlns="http://cpee.org/ns/description/1.0">
      <manipulate id="a5" label="Initialize Time">data.local_clock = Time.now
data.start = Time.now.to_i</manipulate>
      <call id="a6" endpoint="dtcget">
        <parameters>
          <label>Read</label>
          <arguments/>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result">data.data = result['full']</finalize>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations>
          <_generic/>
          <_timing>
            <_timing_weight/>
            <_timing_avg/>
            <explanations/>
          </_timing>
          <_shifting>
            <_shifting_type>Duration</_shifting_type>
          </_shifting>
          <_context_data_analysis>
            <probes/>
            <ips/>
          </_context_data_analysis>
          <report>
            <url/>
          </report>
          <_notes>
            <_notes_general/>
          </_notes>
        </annotations>
        <documentation>
          <input/>
          <output/>
          <implementation>
            <description/>
          </implementation>
          <code>
            <description/>
          </code>
        </documentation>
      </call>
      <parallel wait="1" cancel="last">
        <parallel_branch>
          <call id="a1" endpoint="dtcget">
            <parameters>
              <label>Read</label>
              <arguments>
                <many>true</many>
              </arguments>
            </parameters>
            <code>
              <prepare/>
              <finalize output="result"/>
              <update output="result">if result['partial']
  data.data[result['partial']['group'] + '/' + result['partial']['name']] = result['partial']['value']
end
if result['full']
  data.data = result['full']
end</update>
              <rescue output="result"/>
            </code>
            <annotations>
              <_generic/>
              <_timing>
                <_timing_weight/>
                <_timing_avg/>
                <explanations/>
              </_timing>
              <_shifting>
                <_shifting_type>Duration</_shifting_type>
              </_shifting>
              <_context_data_analysis>
                <probes/>
                <ips/>
              </_context_data_analysis>
              <report>
                <url/>
              </report>
              <_notes>
                <_notes_general/>
              </_notes>
            </annotations>
            <documentation>
              <input/>
              <output/>
              <implementation>
                <description/>
              </implementation>
              <code>
                <description/>
              </code>
            </documentation>
          </call>
        </parallel_branch>
        <parallel_branch>
          <loop mode="post_test" condition="data.duration &lt; 24">
            <_probability>
              <_probability_min/>
              <_probability_max/>
              <_probability_avg/>
            </_probability>
            <call id="a2" endpoint="timeout">
              <parameters>
                <label>Wait</label>
                <arguments>
                  <timeout>1</timeout>
                </arguments>
              </parameters>
              <code>
                <prepare/>
                <finalize output="result">data.duration = Time.now.to_i - data.start</finalize>
                <update output="result"/>
                <rescue output="result"/>
              </code>
              <annotations>
                <_generic/>
                <_timing>
                  <_timing_weight/>
                  <_timing_avg/>
                  <explanations/>
                </_timing>
                <_shifting>
                  <_shifting_type>Duration</_shifting_type>
                </_shifting>
                <_context_data_analysis>
                  <probes/>
                  <ips/>
                </_context_data_analysis>
                <report>
                  <url/>
                </report>
                <_notes>
                  <_notes_general/>
                </_notes>
              </annotations>
              <documentation>
                <input/>
                <output/>
                <implementation>
                  <description/>
                </implementation>
                <code>
                  <description/>
                </code>
              </documentation>
            </call>
            <choose mode="exclusive">
              <alternative condition="data.data['actuators/irrigate']">
                <_probability>
                  <_probability_min/>
                  <_probability_max/>
                  <_probability_avg/>
                </_probability>
                <manipulate id="a7" label="Water">time = 1.0 # hour
soil_amount = 1 # m3
data.new_moisture = data.data['sensors/moisture'] + (
  data.data['actuators/rate'] * 0.001
)  / soil_amount * time
# 10 l per m3 in the default config</manipulate>
              </alternative>
              <otherwise>
                <manipulate id="a8" label="Dry">time = 1.0 # 1 hour
drying = (0.07 + data.data['environment/transpiration']) * 
         (1 - data.data['environment/relative_humidity'])**0.8 * 
         Math.exp(
           0.07 * (data.data['environment/temperature'] - 20)
         )
data.new_moisture = (data.data['sensors/moisture'] - 0.15) * Math.exp(-drying * time) + 0.15</manipulate>
              </otherwise>
            </choose>
            <call id="a4" endpoint="setmoisture">
              <parameters>
                <label>Set Moisture Percentage</label>
                <method>:post</method>
                <arguments>
                  <float>!data.new_moisture</float>
                </arguments>
              </parameters>
              <annotations>
                <_generic/>
                <_timing>
                  <_timing_weight/>
                  <_timing_avg/>
                  <explanations/>
                </_timing>
                <_shifting>
                  <_shifting_type>Duration</_shifting_type>
                </_shifting>
                <_context_data_analysis>
                  <probes/>
                  <ips/>
                </_context_data_analysis>
                <report>
                  <url/>
                </report>
                <_notes>
                  <_notes_general/>
                </_notes>
              </annotations>
              <documentation>
                <input/>
                <output/>
                <implementation>
                  <description/>
                </implementation>
              </documentation>
            </call>
          </loop>
        </parallel_branch>
      </parallel>
    </description>
  </description>
  <transformation>
    <description type="copy"/>
    <dataelements type="none"/>
    <endpoints type="none"/>
  </transformation>
</testset>