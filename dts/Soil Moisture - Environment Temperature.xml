  </attributes>
  <description>
    <description xmlns="http://cpee.org/ns/description/1.0">
      <manipulate id="a4" label="Initialize Time">data.start = Time.now.to_i</manipulate>
      <call id="a5" endpoint="dtcget">
        <parameters>
          <label>Read</label>
          <arguments>
            <item>environment/hour</item>
          </arguments>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result">data.hour = result['full']['environment/hour']</finalize>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations>
          <_generic/>
          <_timing>
            <_timing_weight/>
            <_timing_avg/>
            <explanations/>
          </_timing>
          <_shifting>
            <_shifting_type>Duration</_shifting_type>
