          </_notes>
        </annotations>
        <documentation>
          <input/>
          <output/>
          <implementation>
            <description/>
          </implementation>
          <code>
            <description/>
          </code>
        </documentation>
      </call>
      <parallel wait="1" cancel="last">
        <parallel_branch>
          <call id="a6" endpoint="dtcget">
            <parameters>
              <label>Read</label>
              <arguments>
                <many>true</many>
                <item>environment/hour</item>
              </arguments>
            </parameters>
            <code>
              <prepare/>
              <finalize output="result"/>
              <update output="result">if result['partial']
  if result['partial']['name'] == 'hour'
    data.hour = result['partial']['value']
  end
end
if result['full']
  data.hour = result['full']['environment/hour']
end</update>
              <rescue output="result"/>
            </code>
            <annotations>
              <_generic/>
              <_timing>
                <_timing_weight/>
                <_timing_avg/>
                <explanations/>
              </_timing>
              <_shifting>
                <_shifting_type>Duration</_shifting_type>
              </_shifting>
              <_context_data_analysis>
                <probes/>
                <ips/>
              </_context_data_analysis>
              <report>
                <url/>
              </report>
              <_notes>
                <_notes_general/>
              </_notes>
            </annotations>
            <documentation>
              <input/>
              <output/>
              <implementation>
                <description/>
              </implementation>
              <code>
                <description/>
              </code>
            </documentation>
          </call>
        </parallel_branch>
        <parallel_branch>
          <loop mode="post_test" condition="data.duration &lt; 24">
            <call id="a1" endpoint="timeout">
              <parameters>
                <label>Wait</label>
                <arguments>
                  <timeout>2</timeout>
                </arguments>
              </parameters>
              <code>
                <prepare/>
                <finalize output="result">data.duration = Time.now.to_i - data.start</finalize>
                <update output="result"/>
                <rescue output="result"/>
              </code>
              <annotations>
                <_generic/>
                <_timing>
                  <_timing_weight/>
                  <_timing_avg/>
                  <explanations/>
                </_timing>
                <_shifting>
                  <_shifting_type>Duration</_shifting_type>
                </_shifting>
                <_context_data_analysis>
                  <probes/>
                  <ips/>
                </_context_data_analysis>
                <report>
                  <url/>
                </report>
                <_notes>
                  <_notes_general/>
                </_notes>
              </annotations>
              <documentation>
                <input/>
                <output/>
                <implementation>
                  <description/>
                </implementation>
                <code>
                  <description/>
                </code>
              </documentation>
            </call>
            <_probability>
              <_probability_min/>
              <_probability_max/>
              <_probability_avg/>
            </_probability>
            <manipulate id="a2" label="Calculate Temperature">min_temp = 18
max_temp = 26
random_variation = rand(-0.3..0.3)
daily_variation = 3 * Math.cos((2 * Math::PI) / 24 * (data.hour + 9))
temperature = 22 + daily_variation + random_variation
data.temperature = [[min_temp, temperature].max, max_temp].min</manipulate>
            <call id="a3" endpoint="settemp">
              <parameters>
                <label>Set Temperature</label>
                <arguments>
                  <float>!data.temperature</float>
                </arguments>
              </parameters>
              <annotations>
                <_generic/>
                <_timing>
                  <_timing_weight/>
                  <_timing_avg/>
                  <explanations/>
                </_timing>
                <_shifting>
                  <_shifting_type>Duration</_shifting_type>
                </_shifting>
                <_context_data_analysis>
                  <probes/>
                  <ips/>
                </_context_data_analysis>
                <report>
                  <url/>
                </report>
                <_notes>
                  <_notes_general/>
                </_notes>
              </annotations>
              <documentation>
                <input/>
                <output/>
                <implementation>
                  <description/>
                </implementation>
              </documentation>
            </call>
          </loop>
        </parallel_branch>
      </parallel>
