<testset xmlns="http://cpee.org/ns/properties/2.0">
  <executionhandler>ruby</executionhandler>
  <dataelements/>
  <endpoints>
    <timeout>https://cpee.org/services/timeout.php</timeout>
    <sethour>https-put://cpee.org/dtc/10/environment/hour</sethour>
  </endpoints>
  <attributes>
    <guarded>none</guarded>
    <modeltype>CPEE</modeltype>
    <theme>extended</theme>
    <guarded_id/>
    <info>Soil Moisture - Environment Hour</info>
    <creator>Christine Ashcreek</creator>
    <author>Christine Ashcreek</author>
    <model_uuid>aeabb1b2-0599-4cd4-bbc7-37c622d92a39</model_uuid>
    <model_version/>
    <design_dir>Experiments.dir/Digital Twin Container.dir/CropIrrigation.dir</design_dir>
    <design_stage>development</design_stage>
  </attributes>
  <description>
    <description xmlns="http://cpee.org/ns/description/1.0">
      <manipulate id="a7" label="Initialize Time">data.hour = 1 unless data.hour
data.start = Time.now.to_i</manipulate>
      <loop mode="post_test" condition="data.duration &lt; 24">
        <_probability>
          <_probability_min/>
          <_probability_max/>
          <_probability_avg/>
        </_probability>
        <call id="a1" endpoint="timeout">
          <parameters>
            <label>Wait</label>
            <arguments>
              <timeout>1</timeout>
            </arguments>
          </parameters>
          <code>
            <prepare/>
            <finalize output="result">data.duration = Time.now.to_i - data.start</finalize>
            <update output="result"/>
            <rescue output="result"/>
          </code>
          <annotations>
            <_generic/>
            <_timing>
              <_timing_weight/>
              <_timing_avg/>
              <explanations/>
            </_timing>
            <_shifting>
              <_shifting_type>Duration</_shifting_type>
            </_shifting>
            <_context_data_analysis>
              <probes/>
              <ips/>
            </_context_data_analysis>
            <report>
              <url/>
            </report>
            <_notes>
              <_notes_general/>
            </_notes>
          </annotations>
          <documentation>
            <input/>
            <output/>
            <implementation>
              <description/>
            </implementation>
            <code>
              <description/>
            </code>
          </documentation>
        </call>
        <manipulate id="a2" label="Hour Plus">data.hour += 1
data.hour = 1 if data.hour &gt; 24</manipulate>
        <call id="a6" endpoint="sethour">
          <parameters>
            <label>Set Hour</label>
            <arguments>
              <float>!data.hour</float>
            </arguments>
          </parameters>
          <annotations>
            <_generic/>
            <_timing>
              <_timing_weight/>
              <_timing_avg/>
              <explanations/>
            </_timing>
            <_shifting>
              <_shifting_type>Duration</_shifting_type>
            </_shifting>
            <_context_data_analysis>
              <probes/>
              <ips/>
            </_context_data_analysis>
            <report>
              <url/>
            </report>
            <_notes>
              <_notes_general/>
            </_notes>
          </annotations>
          <documentation>
            <input/>
            <output/>
            <implementation>
              <description/>
            </implementation>
          </documentation>
        </call>
      </loop>
    </description>
  </description>
  <transformation>
    <description type="copy"/>
    <dataelements type="none"/>
    <endpoints type="none"/>
  </transformation>
</testset>