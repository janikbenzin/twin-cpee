            <choose mode="exclusive">
              <alternative condition="data.data['actuators/heat']">
                <manipulate id="a1" label="Heat">time = Time.now - Time.parse(data.local_clock)
y = data.data['sensors/temperature'] + (
  data.data['actuators/energy'] * time
) / (
  data.data['environment/mass'] * data.data['environment/capacity']
)
y = 100 if y &gt; 100
data.new_temperature = y
data.local_clock = Time.now</manipulate>
                <_probability>
